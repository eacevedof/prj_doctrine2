<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BaseUser" table="base_user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="text" column="description" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstName" type="text" column="first_name" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="text" column="last_name" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="text" column="email" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="boLogin" type="text" column="bo_login" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="boPassword" type="text" column="bo_password" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="language" type="text" column="language" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idLanguage" type="integer" column="id_language" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="pathPicture" type="text" column="path_picture" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idProfile" type="integer" column="id_profile" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
