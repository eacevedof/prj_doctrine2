<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ComSync" table="com_sync">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idDeviceUser" type="integer" column="id_device_user" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lastDate" type="text" column="last_date" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idTypeStatus" type="integer" column="id_type_status" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
