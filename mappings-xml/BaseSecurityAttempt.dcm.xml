<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BaseSecurityAttempt" table="base_security_attempt">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="processflag" type="text" column="processflag" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="insertPlatform" type="text" column="insert_platform" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="insertUser" type="text" column="insert_user" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="insertDate" type="text" column="insert_date" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="remoteIp" type="text" column="remote_ip" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="login" type="text" column="login" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="text" column="password" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
